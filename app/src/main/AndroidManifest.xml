        <activity android:name="com.example.ezbillmanager.activity.newlabel"
            android:exported="true"/>
        <activity android:name="com.example.ezbillmanager.activity.Accountnext1"
            android:exported="true"/>
        <activity android:name="com.example.ezbillmanager.activity.Accountnext4"
            android:exported="true"/>
        <activity android:name="com.example.ezbillmanager.activity.Accountnext2"
            android:exported="true"/>
        <activity android:name="com.example.ezbillmanager.activity.Accountnext3"
            android:exported="true"/>

        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
